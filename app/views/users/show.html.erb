<h2><%= @user.username %></h2>
<h3>Posts:</h3>
<ul>
  <% @user.posts.each do |post| %>
  <li><%= link_to post.title, post_path(post) %></li>
  <% end %> 
</ul>
<h3>Comments</h3>
<ul>
<% @user.comments.each do |comment| %>
  <li><%= link_to comment.post, comment_path(comment) %></li>
<% end %>
</ul>
